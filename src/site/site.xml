<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.8.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.8.0 https://maven.apache.org/xsd/decoration-1.8.0.xsd"
         name="${project.name} ${project.version}">
    <publishDate position="left" format="dd-MMMM-yyyy" />
    <version position="left" />
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.7</version>
    </skin>
    <bannerLeft>
        <name>${project.organization.name}</name>
        <src>images/logo.png</src>
        <href>${project.organization.url}</href>
    </bannerLeft>
    <bannerRight>
        <name>${project.name}</name>
        <href>${project.url}</href>
    </bannerRight>
    <poweredBy>
        <logo href="https://travis-ci.com/B3Partners/flamingo-ibis"
              img="https://travis-ci.com/B3Partners/flamingo-ibis.svg?branch=master"
              name="Travis-CI Build status (master branch)" />
        <logo href="http://192.168.1.57:8080/job/B3Partners/job/flamingo-ibis/job/master/"
              img="http://192.168.1.57:8080/buildStatus/icon?job=B3Partners%2Fflamingo-ibis%2Fmaster&amp;style=plastic"
              name="Jenkins Build status (master branch)" />
        <logo href="https://github.com/flamingo-geocms"
              img="https://avatars1.githubusercontent.com/u/4628473?v=3&amp;s=200"
              name="Flamingo Geo CMS" />
    </poweredBy>
    <body>
        <head>
            <![CDATA[<link rel="shortcut icon" href="favicon.ico" type="image/x-icon" />]]>
            <![CDATA[<link rel="icon" href="favicon.ico" type="image/x-icon" />]]>
        </head>
        <links>
            <item name="Issues dashboard" href="https://github.com/B3Partners/flamingo-ibis/issues/" />
        </links>
        <menu name="Information" inherit="top">
            <item name="About" href="/index.html" />
            <item name="Deployment notes" href="/notes.html" />
            <item name="Release notes" href="/releasenotes.html" />
            <item name="Developer Notes" href="/development.html" />
            <item name="Flamingo Wiki" href="https://github.com/flamingo-geocms/flamingo/wiki/" />
        </menu>
        <menu ref="modules" />
        <menu ref="reports" />
    </body>
</project>
